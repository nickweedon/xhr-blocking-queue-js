{
  "name": "xhr-blocking-queue-js",
  "version": "0.0.21",
  "authors": [
    "Nick Weedon <nick@weedon.org.au>"
  ],
  "description": "Javascript XHR proxy library which provides an easily overridable 'adaptor' class that wraps the underlying XHR object (which may be a COM object on old IE browsers)",
  "main": "dist/xhr-blocking-queue-js.min.js",
  "moduleType": [
    "amd"
  ],
  "keywords": [
    "adaptor",
    "proxy",
    "wrapper",
    "interceptor",
    "ajax",
    "xhr",
    "xmlHttpRequest"
  ],
  "license": "MIT",
  "homepage": "http://git.redwyvern.com/SAML/xhr-blocking-queue-js",
  "repository": {
    "type": "git",
    "url": "git://git.redwyvern.com/SAML/xhr-blocking-queue-js"
  },
  "ignore": [
    ".idea",
    "design",
    "doc_resource",
    "src",
    "test",
    ".gitignore",
    "Gruntfile.js",
    "README.md",
    "jsdoc.conf.json",
    "karma.conf.js",
    "package.json"
  ],
  "dependencies": {
    "xhr-adaptor-js": "~0.0.0"
  },
  "devDependencies": {
    "requirejs": "~2.1.15",
    "jquery": "~2.1.4"
  }
}
